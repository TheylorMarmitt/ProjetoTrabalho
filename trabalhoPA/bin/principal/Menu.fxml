<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="bpPrincipal" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="principal.MenuController">
   <top>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </top>
   <left>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnAlugar" layoutX="54.0" layoutY="61.0" mnemonicParsing="false" onAction="#alugar" prefHeight="25.0" prefWidth="137.0" text="Alugar" />
         </children>
      </Pane>
   </left>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <MenuButton fx:id="mnCadastrar" alignment="CENTER" layoutX="-23.0" layoutY="65.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="160.0" text="Cadastrar" textAlignment="CENTER">
              <items>
                <MenuItem fx:id="mniCarro" mnemonicParsing="false" onAction="#cadastroCarro" text="Carro" />
                <MenuItem fx:id="mniCliente" mnemonicParsing="false" onAction="#cadastroCliente" text="Cliente" />
                  <MenuItem fx:id="mniFilial" mnemonicParsing="false" onAction="#cadastroFilial" text="Filial" />
                  <MenuItem fx:id="mniVendedor" mnemonicParsing="false" onAction="#cadastroVendedor" text="Vendedor" />
              </items>
            </MenuButton>
         </children>
      </Pane>
   </right>
   <bottom>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
