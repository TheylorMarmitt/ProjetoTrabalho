<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="principal.AlugarController">
   <children>
      <ComboBox fx:id="cbcarro" layoutX="496.0" layoutY="133.0" prefWidth="150.0" promptText="Carros" />
      <ComboBox fx:id="cbVendendor" layoutX="301.0" layoutY="134.0" prefWidth="150.0" promptText="Vendedor" />
      <ComboBox fx:id="cbFilial" layoutX="117.0" layoutY="133.0" prefWidth="150.0" promptText="Filial" />
      <TableView fx:id="tblCliente" layoutX="101.0" layoutY="181.0" onMouseClicked="#selecionaCliente" prefHeight="200.0" prefWidth="575.0">
         <columns>
            <TableColumn fx:id="tbcCodigo" prefWidth="75.0" text="Codigo" />
            <TableColumn fx:id="tbcNome" prefWidth="495.0" text="Nome" />
         </columns>
      </TableView>
      <Button fx:id="btnAlugar" layoutX="309.0" layoutY="399.0" mnemonicParsing="false" onAction="#Alugar" prefHeight="32.0" prefWidth="159.0" text="Alugar" />
      <Label fx:id="lblMensagem" layoutX="372.0" layoutY="476.0" />
      <Label layoutX="117.0" layoutY="89.0" text="Codigo: " />
      <TextField fx:id="tfCodigo" layoutX="169.0" layoutY="84.0" />
   </children>
</AnchorPane>
